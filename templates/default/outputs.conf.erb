[tcpout]
defaultGroup = <%= @indexers_group1_name %> <% unless @indexers_group2_splunk_servers.empty? %> ,<%= @indexers_group2_name %> <% end %>
disabled=false

[tcpout:<%= @indexers_group1_name %>
server=<%= @indexers_group1_splunk_servers %>
<% @indexers_group1_outputs_conf.each_pair do |name, value| -%>
<%= name %> = <%= value %>
<% end -%>

<% unless @indexers_group2_splunk_servers.empty? %>
[tcpout:<%= @indexers_group2_name %>
server=<%= @indexers_group2_splunk_servers %>
<% @indexers_group2_outputs_conf.each_pair do |name, value| -%>
<%= name %> = <%= value %>
<% end -%>
<% end %>
